{
  "name": "svelte-web",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite dev --port 5173 --open",
    "build": "vite build",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --ignore-path=../../packages/svelte-eslint-config/.prettierignore --config=../../packages/svelte-eslint-config/.prettierrc . && eslint \"src\"",
    "format": "prettier --write --ignore-path=../../packages/svelte-eslint-config/.prettierignore --config=../../packages/svelte-eslint-config/.prettierrc ."
  },
  "dependencies": {
    "svelte-ui": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "latest",
    "@sveltejs/kit": "latest",
    "eslint-config-svelte": "workspace:*",
    "svelte": "^4.2.2",
    "svelte-check": "^3.2.0",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.3.1",
    "typescript": "^4.7.2",
    "vite": "^4.0.0"
  },
  "type": "module"
}
