{
    "name": "monwoo/mws-moon-manager-bundle",
    "version": "2.0.0",
    "description": "Open Source Monwoo MoonManager time tracking tool",
    "type": "symfony-bundle",
    "license": "Apache-2.0_monwoo",
    "authors": [
        {
            "name": "Miguel Monwoo",
            "email": "service@monwoo.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "phpoffice/phpspreadsheet": "^1.29",
        "symfony/framework-bundle": "^6.3",
        "symfony/flex": "^2.3",
        "symfony/console": "6.3.*",
        "symfony/runtime": "6.3.*"
    },
    "autoload": {
        "psr-4": {
            "MWS\\MoonManagerBundle\\": "src/",
            "App\\": "app/"
        }
    },
    "extra": {
        "thanks": {
            "name": "Monwoo/web-starters-free",
            "url": "https://github.com/Monwoo/web-starters-free"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "6.3.*"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
